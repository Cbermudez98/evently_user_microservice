{
  "name": "evently_user_microservice",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "jest",
    "test:e2e": "jest --config ./jest-e2e.json --detectOpenHandles",
    "test:dev": "jest --coverage",
    "start:dev": "ts-node-dev src/Index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- -d ./src/app/Typeorm.ts",
    "migration:generate": "npm run typeorm -- -d ./src/app/Typeorm.ts migration:generate ./src/migrations/$npm_config_name",
    "migration:create": "npm run typeorm -- migration:create ./src/migrations/%npm_config_name%",
    "migration:revert": "npm run typeorm -- -d ./src/app/Typeorm.ts migration:revert",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\"",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@jest-mock/express": "^2.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/bunyan": "^1.8.11",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.20",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "better-sqlite3": "^9.4.3",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/morgan": "^1.9.9",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "inversify": "^6.0.2",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.1",
    "reflect-metadata": "^0.2.1",
    "ts-node": "^10.9.2",
    "tslog": "^4.9.2",
    "typeorm": "^0.3.20"
  }
}
